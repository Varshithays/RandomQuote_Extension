{
    "manifest_version": 3,
    "name": "Random Quote Generator",
    "version": "1.0",
    "description": "A simple Chrome extension that generates random quotes",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png"
    },
    "action": {
      "default_icon": "icon16.png",
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
  }
  